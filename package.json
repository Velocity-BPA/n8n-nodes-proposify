{
  "name": "n8n-nodes-proposify",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Proposify providing proposal management, template operations, prospect tracking, and e-signature automation with 10 resources and 70+ operations.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-proposify.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-proposify#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-proposify/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "proposify",
    "proposal",
    "sales",
    "e-signature",
    "document",
    "automation",
    "crm",
    "quotes"
  ],
  "main": "dist/nodes/Proposify/Proposify.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ProposifyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Proposify/Proposify.node.js",
      "dist/nodes/Proposify/ProposifyTrigger.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
